{
    "name": "Julia Utility",
    "type": "dark",
    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#5577ff"
            }
        },
        {
            "name": "Function/macro keywords",
            "scope": [
                "keyword.other.function",
                "keyword.other.macro"
            ],
            "settings": {
                "foreground": "#ffb057"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#85cdfa"
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "keyword.other.const",
                "keyword.other.end",
                "keyword.control"
            ],
            "settings": {
                "foreground": "#d66969"
            }
        },
        {
            "name": "Macro",
            "scope": [
                "call.macro",
                "entity.name.macro"
            ],
            "settings": {
                "foreground": "#f0e68c"
            }
        },
        {
            "name": "Interpolated string",
            "scope": [
                "string.interpolated"
            ],
            "settings": {
                "foreground": "#ee9c38"
            }
        },
        {
            "name": "String",
            "scope": [
                "string.quoted.double",
                "string.quoted.triple"
            ],
            "settings": {
                "foreground": "#a9e67b"
            }
        },
        {
            "name": "Character",
            "scope": [
                "string.quoted.single"
            ],
            "settings": {
                "foreground": "#7fffd4"
            }
        },
        {
            "name": "Regex string",
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#48d1cc"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "operator"
            ],
            "settings": {
                "foreground": "#5ccbff"
            }
        },
        {
            "name": "Literals",
            "scope": [
                "constant.numeric",
                "constant.language"
            ],
            "settings": {
                "foreground": "#ffaf53"
            }
        }
    ],
    "colors": {
        "activityBar.activeBackground": "#9679ff38",
        "activityBar.background": "#010103",
        "activityBar.border": "#00384B",
        "activityBar.foreground": "#ffaf53dd",
        "button.hoverBackground": "#00384B",
        "dropdown.background": "#002936",
        "dropdown.border": "#00384B",
        "dropdown.listBackground": "#002936",
        "editor.background": "#010103",
        "editor.findMatchBackground": "#00e1ff2a",
        "editor.findMatchBorder": "#00ffff",
        "editor.findMatchHighlightBackground": "#00e1ff34",
        "editorGroup.border": "#00384b00",
        "editorGroup.dropBackground": "#00ffff23",
        "editorGroup.emptyBackground": "#010103",
        "editorGroupHeader.tabsBackground": "#010103",
        "editorGroupHeader.tabsBorder": "#00384B",
        "editorHoverWidget.background": "#001920",
