        "editorSuggestWidget.highlightForeground": "#ffaf53dd",
        "editorSuggestWidget.selectedBackground": "#00ffff0a",
        "editorWidget.background": "#010103",
        "editorWidget.resizeBorder": "#6eeeff",
        "icon.foreground": "#6eeeff",
